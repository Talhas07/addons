<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <!-- This file ensures the repair sequence exists if not already present -->
    <!-- The standard repair module should already have this, but we keep it for safety -->
    <record id="seq_repair" model="ir.sequence">
        <field name="name">Repair Order</field>
        <field name="code">repair.order</field>
        <field name="prefix">RO</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
